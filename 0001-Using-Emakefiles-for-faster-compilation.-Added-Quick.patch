+	height: 58px;
+	margin: 0;
+	margin-right: 20px;
+	padding-top: 2px;
+	background: #514F42;
+	line-height: normal;
+	text-transform: uppercase;
+	text-align: center;
+	font-size: 10px;
+	font-weight: bold;
+	color: #FFFFFF;
+}
+
+.post .date b {
+	margin: 0;
+	padding: 0;
+	display: block;
+	margin-top: -5px;
+	font-size: 40px;
+}
+
+.post .title {
+	margin: 0;
+	padding: 0px 0 0 0;
+	margin-left: 10px;
+	padding-left: 50px;
+	font-size: 1.8em;
+}
+
+.post .title h2 {
+	padding: 0;
+	margin: 0;
+}
+
+.post .hr1 {
+}
+
+.post .meta {
+	margin: 0 0 30px 20px;
+	padding: 0;
+	color: #979680;
+	line-height:normal;
+}
+
+.post .meta a {
+	color: #828170;
+}
+
+.post .entry {
+	margin: 0;
+	padding: 0 0 20px 0;
+	border-bottom: 1px dashed #666633;
+}
+
+/* Sidebar */
+
+.sidebar {
+	border-left: solid 1px #AFAE99;
+	padding: 20px 0px 20px 20px;
+	float: right;
+	width: 220px;
+	margin-top: 20px;
+	margin-bottom: 20px;
+}
+
+.sidebar h3 {
+	font-size: 12px;
+	padding: 2px 0px;
+	border-top: 1px dashed #666633;
+	border-bottom: 1px dashed #666633;
+}
+
+/* Footer */
+
+.wrapper2 {
+	background: #422C21;
+}
+
+.footer {
+	position: relative;
+	width: 800px;
+	margin: 0px auto;
+	padding: 40px 0px;
+	color: #FFFFFF;
+}
+
+.footer .col {
+	display: block;
+	float: left;
+	width: 250px;
+	margin-right: 15px;
+}
+
+.footer a {
+	font-family: Tahoma, Arial, Helvetica, sans-serif;
+	text-decoration: none;
+	color:#FD4;
+}
+
+.footer li {
+	list-style-type: square;
+	margin-bottom: 8px;
+}
+
+.footer a:HOVER {
+	text-decoration: underline;
+}
+
+/* Legal */
+
+.legal {
+	clear: both;
+	margin: 0;
+	padding: 10px 0;
+	text-align: center;
+	font-family: Tahoma, Arial, Helvetica, sans-serif;
+	font-size: 10px;
+	color: #525252;
+	background: black;
+}
+
+.legal a {
+	border-bottom: 1px dotted #939393;
+	text-decoration: none;
+	color: #939393;
+}
