+	height: 3px;
+	border: 0px;
+	background-color: #AFAC99;
+	margin: 10px 0px 10px 0px;
+}
+
+/* Header */
+
+.header {
+	position: relative;
+	width: 800px;
+	height: 115px;
+	margin: 0px auto;
+}
+
